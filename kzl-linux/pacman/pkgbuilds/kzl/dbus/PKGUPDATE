stable_version=$(curl -L --stderr - https://wiki.freedesktop.org/www/Software/dbus/ | grep '<strong>stable</strong>' | grep -Po '(?<=dbus )\d\.\d\d(?=\.x)')
new_version=$(curl -L --stderr - https://gitlab.freedesktop.org/dbus/dbus/-/tags | grep -Po "(?<=dbus-)$stable_version.\d\d?" | sort -V | tail -1)